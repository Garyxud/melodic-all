    package -hardware CYGPKG_DEVICES_WALLCLOCK_DALLAS_DS12887 current ;
    package -hardware CYGPKG_DEVICES_WALLCLOCK_I386_PC current ;
    package CYGPKG_HAL current ;
    package CYGPKG_INFRA current ;
