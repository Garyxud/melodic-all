<?xml version="1.0"?>
<package>

  <name>multisense_ros</name>
  <version>4.0.4</version>
  <description>multisense_ros</description>
  <maintainer email="support@carnegierobotics.com">Carnegie Robotics</maintainer>
  <license>BSD</license>

  <url type="website">https://github.com/carnegierobotics/multisense_ros</url>

  <buildtool_depend>catkin</buildtool_depend>
