<package>
  <name>fsrobo_r_description</name>
  <version>0.7.1</version>
  <description>
    <p>URDF Description package for FSRobo-R</p>
    <p>This package contains configuration data, 3D models and launch files
for FSRobo-R robot</p>
  </description>

  <author email="F-ROSROBO@fsi.co.jp">F-ROSROBO</author>
  <maintainer email="F-ROSROBO@fsi.co.jp">F-ROSROBO</maintainer>
  <license>BSD</license>
  <url type="website">http://wiki.ros.org/fsrobo_r_description</url>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roslaunch</build_depend>
  <run_depend>robot_state_publisher</run_depend>
  <run_depend>rviz</run_depend>
  <run_depend>joint_state_publisher</run_depend>
  <run_depend>gazebo</run_depend>
  <export>
    <architecture_independent />
  </export>
</package>
