<package>
  <name>fsrobo_r_trajectory_filters</name>
  <version>0.7.1</version>
  <description>
    plugins for filtering trajectories.
  </description>
  <author email="F-ROSROBO@fsi.co.jp">F-ROSROBO</author>
  <maintainer email="F-ROSROBO@fsi.co.jp">F-ROSROBO</maintainer>
  <license>BSD</license>
  <url type="website">http://wiki.ros.org/fsrobo_r_trajectory_filters</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>trajectory_msgs</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>moveit_core</build_depend>
  <build_depend>moveit_ros_planning</build_depend>
  <build_depend>orocos_kdl</build_depend>

  <run_depend>trajectory_msgs</run_depend>
  <run_depend>pluginlib</run_depend>
  <run_depend>moveit_core</run_depend>
  <run_depend>moveit_ros_planning</run_depend>
  <run_depend>orocos_kdl</run_depend>

  <export>	
    <moveit_core plugin="${prefix}/planning_request_adapters_plugin_description.xml"/> 
